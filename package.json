{
  "name": "scanning-app",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn dev:server & yarn dev:svelte",
    "dev:svelte": "svelte-kit dev",
    "dev:server": "cd mock-server && yarn dev && cd ..",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "rails-build": "yarn build && yarn rails-build:erb",
    "rails-build:erb": "yarn node bin/erb-inject.js ruby-injection-config.txt"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@poppanator/sveltekit-svg": "^0.3.1",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.4.18",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "1.0.0-next.303",
    "@types/faker": "^6.6.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vaxxnz/nzcp": "1.2.0",
    "babel-loader": "^8.2.4",
    "dayjs": "^1.11.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "faker": "^5.5.3",
    "fuse.js": "^6.4.6",
    "modern-svelte-qr-scanner": "^1.3.6",
    "normalize-strings": "^1.1.1",
    "prettier": "^2.6.1",
    "prettier-plugin-svelte": "^2.5.1",
    "sass": "^1.49.9",
    "stampit": "^4.3.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-frappe-charts": "^1.9.1",
    "svelte-loader": "^3.1.2",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-multiselect": "^4.0.3",
    "svelte-preprocess": "^4.10.4",
    "svelte-select": "^4.4.6",
    "svelte-simple-modal": "^1.3.1",
    "svelte-steps": "^2.3.5",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "visibilityjs": "^2.0.2"
  },
  "type": "module",
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "jsqr": "^1.4.0",
    "svelte-chartjs": "^1.1.4"
  }
}
