{
	"name": "scanning-app",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"private": "true",
	"scripts": {
		"dev": "concurrently --raw --default-input-target 1 --kill-others 'yarn dev:server' 'yarn dev:svelte'",
		"dev:svelte": "vite dev",
		"dev:server": "cd mock-server && yarn dev && cd ..",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"rails-build": "yarn build && yarn rails-build:erb",
		"rails-build:erb": "yarn node bin/erb-inject.js ruby-injection-config.txt"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@faker-js/faker": "^7.3.0",
		"@poppanator/sveltekit-svg": "^0.3.4",
		"@storybook/addon-actions": "^6.5.9",
		"@storybook/addon-essentials": "^6.5.9",
		"@storybook/addon-links": "^6.5.9",
		"@storybook/addon-svelte-csf": "^2.0.5",
		"@storybook/svelte": "^6.5.9",
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"@sveltejs/kit": "1.21.0",
		"@types/faker": "^6.6.9",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.30.5",
		"@typescript-eslint/parser": "^5.30.5",
		"@vaxxnz/nzcp": "1.2.0",
		"babel-loader": "^8.2.5",
		"concurrently": "^8.0.0",
		"dayjs": "^1.11.3",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fuse.js": "^6.6.2",
		"modern-svelte-qr-scanner": "^1.3.6",
		"normalize-strings": "^1.1.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.53.0",
		"stampit": "^4.3.2",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-frappe-charts": "^1.9.2",
		"svelte-loader": "^3.1.3",
		"svelte-loading-spinners": "^0.3.0",
		"svelte-multiselect": "^5.0.3",
		"svelte-preprocess": "^4.10.7",
		"svelte-select": "^4.4.6",
		"svelte-simple-modal": "^1.4.1",
		"svelte-steps": "^2.3.5",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2",
		"visibilityjs": "^2.0.2",
		"vite": "^2.9.14"
	},
	"type": "module",
	"packageManager": "yarn@1.22.19",
	"dependencies": {
		"jsqr": "^1.4.0",
		"svelte-chartjs": "^1.1.4"
	}
}
